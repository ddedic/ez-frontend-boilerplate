{
  "name": "EzFrontendBoilerplate",
  "version": "1.0.0",
  "description": "",
  "author": "Dominik Ager",
  "license": "ISC",
  "scripts": {
    "stylelint": "node_modules/sass-lint/bin/sass-lint.js -v app/Resources/public/scss/**/*.*",
    "compilecss": "node-sass app/Resources/public/scss/app.scss web/assets/css/app.css",
    "postcss": "node_modules/postcss-cli/bin/postcss --use autoprefixer -b 'last 2 versions' -r web/assets/css/app.css",
    "buildjs": "node_modules/.bin/webpack",
    "buildcss": "npm run stylelint && npm run compilecss && npm run postcss",
    "watch": "watch 'npm run build' app/Resources/public/scss/. & watch 'npm run buildjs' app/Resources/public/js/.",
    "build": "npm run buildcss && npm run buildjs",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.22.0",
    "bootstrap-sass": "^3.3.7",
    "csscomb": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.7.1",
    "glob": "^7.1.1",
    "grunt": "^1.0.2",
    "grunt-csscomb": "^3.1.1",
    "husky": "^0.14.3",
    "jquery": "^3.2.1",
    "lightgallery": "^1.6.6",
    "lint-staged": "^7.0.0",
    "mixitup": "^3.2.2",
    "node-sass": "^3.13.1",
    "postcss-cli": "^2.6.0",
    "sass-lint": "^1.12.1",
    "script-loader": "^0.7.0",
    "slick-carousel": "^1.8.1",
    "stylelint": "^8.4.0",
    "watch": "^0.19.2",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-webfonts": "^1.0.6",
    "bootstrap": "^4.1.0",
    "jquery": "^3.3.1",
    "lightgallery": "^1.6.9",
    "popper.js": "^1.14.3",
    "slick-carousel": "^1.8.1"
  }
}
